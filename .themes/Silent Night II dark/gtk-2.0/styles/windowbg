style "windowbg"
{
   engine "pixmap"
   {
        image
        {
                function = FLAT_BOX
		recolorable		= TRUE
                file = "Others/bg.png"
                border = {1, 1, 1, 1}
		stretch = FALSE
		overlay_file		= "Others/bg1.png"
		overlay_border		= { 2, 2, 2, 2 }
		overlay_stretch		= TRUE
        }
   }

}

class "GtkWindow" style "windowbg"
